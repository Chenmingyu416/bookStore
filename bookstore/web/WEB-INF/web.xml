<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>DoSaveEditTitle</servlet-name>
        <servlet-class>servlet.DoSaveEditTitle</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DoSaveEditTitle</servlet-name>
        <url-pattern>/DoSaveEditTitle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ToEditTitle</servlet-name>
        <servlet-class>servlet.ToEditTitle</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ToEditTitle</servlet-name>
        <url-pattern>/ToEditTitle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DoAddTitle</servlet-name>
        <servlet-class>servlet.DoAddTitle</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DoAddTitle</servlet-name>
        <url-pattern>/DoAddTitle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ToViewTitle</servlet-name>
        <servlet-class>servlet.ToViewTitle</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ToViewTitle</servlet-name>
        <url-pattern>/ToViewTitle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddTitleToCart</servlet-name>
        <servlet-class>servlet.AddTitleToCart</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddTitleToCart</servlet-name>
        <url-pattern>/AddTitleToCart</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DoOrder</servlet-name>
        <servlet-class>servlet.DoOrder</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DoOrder</servlet-name>
        <url-pattern>/DoOrder</url-pattern>
    </servlet-mapping>
</web-app>